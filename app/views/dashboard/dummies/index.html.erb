<p id="notice"><%= notice %></p>

<h1>Listing Dashboard Dummies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dashboard_dummies.each do |dashboard_dummy| %>
      <tr>
        <td><%= dashboard_dummy.name %></td>
        <td><%= link_to 'Show', dashboard_dummy %></td>
        <td><%= link_to 'Edit', edit_dashboard_dummy_path(dashboard_dummy) %></td>
        <td><%= link_to 'Destroy', dashboard_dummy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dummy', new_dashboard_dummy_path %>
